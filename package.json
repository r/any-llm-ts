{
  "name": "any-llm-ts",
  "version": "0.2.0",
  "description": "A unified TypeScript interface for LLM providers using official SDKs - supports OpenAI, Anthropic, Ollama, and more",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.js",
      "default": "./dist/providers/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "llm",
    "ai",
    "openai",
    "anthropic",
    "claude",
    "gpt",
    "ollama",
    "llamafile",
    "typescript",
    "unified-api",
    "official-sdk"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/any-llm-ts"
  },
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/any-llm-ts/issues"
  },
  "homepage": "https://github.com/YOUR_USERNAME/any-llm-ts#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "openai": "^4.77.0",
    "@anthropic-ai/sdk": "^0.35.0",
    "ollama": "^0.5.11"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}

